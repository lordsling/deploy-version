{
    "name": "nlmenke/deploy-version",
    "description": "Automatically update application versions by deploying.",
    "keywords": ["laravel", "deploy", "deployment", "version", "versioning", "semver"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Menke",
            "email": "nick@nlmenke.net"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "NLMenke\\DeployVersion\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "NLMenke\\DeployVersion\\DeployVersionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
