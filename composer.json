{
    "name": "nlmenke/deploy-version",
    "description": "Automatically update application versions by deploying.",
    "keywords": ["laravel", "deploy", "deployment", "version"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Menke",
            "email": "nick@nlmenke.net"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^5.6",
        "illuminate/database": "^5.6",
        "illuminate/filesystem": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "NLMenke\\DeployVersion\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.0-dev"
        },
        "laravel": {
            "providers": [
                "NLMenke\\DeployVersion\\DeployVersionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
